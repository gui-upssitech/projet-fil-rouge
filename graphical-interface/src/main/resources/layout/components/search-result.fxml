<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" spacing="12.0" styleClass="search-result" stylesheets="@../../stylesheets/search-result.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="icon" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/icons/file-text.png" />
         </image>
      </ImageView>
      <Label fx:id="file_name" maxWidth="1.7976931348623157E308" text="Result name" HBox.hgrow="ALWAYS" />
      <Label fx:id="confidence" text="5%">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
   </padding>
</HBox>
