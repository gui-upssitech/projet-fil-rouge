<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.miniteldo.search.controller.ParameterController">
   <children>
      <VBox fx:id="settingsBox" alignment="CENTER" fillWidth="false" spacing="12.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
              <Label alignment="CENTER" text="Paramètres">
                  <font>
                      <Font name="System Bold" size="28.0" />
                  </font>
               <VBox.margin>
                  <Insets bottom="24.0" />
               </VBox.margin>
              </Label>
            <AnchorPane onMouseClicked="#onThemeBarClicked" prefWidth="360.0" styleClass="setting">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="12.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/icons/layout.png" />
                           </image>
                        </ImageView>
                        <Label text="Thème visuel" />
                     </children>
                  </HBox>
                  <ImageView fx:id="moonIcon" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <image>
                        <Image url="@../images/icons/moon.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="sunIcon" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <image>
                        <Image url="@../images/icons/sun.png" />
                     </image>
                  </ImageView>
               </children>
               <padding>
                  <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
               </padding>
            </AnchorPane>
            <HBox alignment="CENTER_LEFT" onMouseClicked="#onAdminButton" prefWidth="360.0" spacing="12.0" styleClass="setting">
               <children>
                  <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/icons/lock.png" />
                     </image>
                  </ImageView>
                  <Label text="Mode Administrateur" />
               </children>
               <padding>
                  <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefWidth="360.0" spacing="12.0" styleClass="setting">
               <children>
                  <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/icons/save.png" />
                     </image>
                  </ImageView>
                  <Label text="Mes recherches sauvegardées" />
               </children>
               <padding>
                  <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
      <ImageView fitHeight="24.0" fitWidth="24.0" onMouseClicked="#onReturnButton" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="16.0" AnchorPane.topAnchor="16.0">
         <image>
            <Image url="@../images/icons/arrow-left.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
   <stylesheets>
      <URL value="@../stylesheets/theme.css" />
      <URL value="@../stylesheets/parameters.css" />
   </stylesheets>
</AnchorPane>
